{
   "name": "elevate",
   "version": "0.1.0",
   "private": true,
   "main": "main.js",
   "scripts": {
      "-dev:test:run:unit": "CI=true npm run test:run:unit:run --watch",
      "build-storybook": "build-storybook",
      "build:next": "next build",
      "build:types": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only schema",
      "build": "run-s build:types build:next",
      "commit": "npx git-cz",
      "dev:app:db:studio": "dotenv -e .env.local prisma studio",
      "dev:app:db:watch": "prisma generate --watch",
      "dev:app:desktop": "npm run dev:app:pre && dotenv -e .env.local electron-forge start",
      "dev:app:pre": "./scripts/development/setup.sh",
      "dev:app:web": "npm run dev:app:pre && nf --env .env.local start web",
      "dev:components": "start-storybook -p 6006",
      "dev:test:pre:env": "node ./scripts/create-test-env",
      "dev:test:pre": "run-s dev:test:pre:*",
      "dev:test:run:component": "run-s dev:test:pre && run-p test:open:component",
      "dev:test:run:e2e": "run-s dev:test:pre && nf --env .env.test.local start web & scripts/wait-on-server.js && run-s test:open:e2e",
      "dev:test:run": "run-p dev:test:run:*",
      "dev:test": "run-s dev:test:pre dev:test:run",
      "graphql-generate": "apollo client:codegen --watch --target typescript --localSchemaFile generated/schema.graphql --outputFlat --includes \"pages/**\" generated",
      "init:patch": "patch-package",
      "init:prisma": "prisma generate",
      "init": "node ./scripts/init",
      "lint:check:errors": "eslint --no-error-on-unmatched-pattern './{components,cypress,hooks,pages,schema,types,utils}/*.{js,jsx,ts,tsx}'",
      "lint:check:format": "prettier './**/*.{js,jsx,ts,tsx,css,md*,json,yml,yaml}' --check",
      "lint:check:gherkin": "gherkin-lint  --ignore 'out/**'",
      "lint:check": "run-s lint:check:*",
      "lint:fix:errors": "npm run lint:check:errors -- --fix",
      "lint:fix:format:staged": "pretty-quick --staged",
      "lint:fix:format": " npm run lint:check:format -- --write",
      "lint:fix": "run-s lint:fix:format lint:fix:errors",
      "make:dist": "electron-forge make",
      "make:package": "electron-forge package",
      "postinstall": "npm run init",
      "prepare": "husky install",
      "serve": "node ./server.js",
      "start": "ntl --descriptions-only --autocomplete",
      "test:coverage": "nyc check-coverage",
      "test:open:component": "cypress open-ct --browser electron",
      "test:open:e2e": "cypress open --browser electron",
      "test:post": "run-s test:report test:coverage",
      "test:pre:init": "mkdir reports .nyc_output || true",
      "test:pre:purge": "rm -rdf prisma/test.db storybook-static jest-coverage cypress-coverage reports .nyc_output coverage .next || true",
      "test:pre": "run-s test:pre:purge test:pre:init lint:check build",
      "test:report:merge": "npx nyc merge reports",
      "test:report:move": "mv coverage.json .nyc_output/out.json",
      "test:report:run": "nyc report --reporter lcov --report-dir coverage",
      "test:report": "run-s test:report:merge test:report:move test:report:run",
      "test:run-wait": "./scripts/run-e2e.js",
      "test:run:component": "cypress run-ct && mv cypress-coverage/coverage-final.json reports/from-cypress-component.json",
      "test:run:e2e": "./scripts/wait-on-server.js && cypress run && mv cypress-coverage/coverage-final.json reports/from-cypress-e2e.json",
      "test:run:unit:post": "mv jest-coverage/coverage-final.json reports/from-jest.json",
      "test:run:unit:run": "jest --passWithNoTests",
      "test:run:unit": "run-s test:run:unit:run test:run:unit:post",
      "test:run": "run-s test:run:*",
      "test": "run-s test:pre test:run-wait test:post",
      "ts-node": "ts-node --compiler-options \"{\\\"module\\\":\\\"commonjs\\\"}\""
   },
   "ntl": {
      "descriptions": {
         "build": "Builds the project",
         "commit": "Interactive git commit",
         "dev:app:db:studio": "Prisma studio",
         "dev:app:db:watch": "Auto generate types for Prisma client",
         "dev:app:desktop": "Dev server (with Electron)",
         "dev:app:web": "Dev server (only browser)",
         "dev:components": "Component development (Storybook)",
         "dev:test:run:component": "Component Testing (Cypress)",
         "dev:test:run:e2e": "E2E Testing (Cypress)",
         "test:coverage": "Coverage report",
         "test": "Test and generate coverage report (CI)"
      }
   },
   "dependencies": {
      "@apollo/client": "^3.3.19",
      "@next-auth/prisma-adapter": "0.4.4-canary.64",
      "@nexus/schema": "^0.20.1",
      "@prisma/client": "2.24.1",
      "@sentry/nextjs": "^6.5.1",
      "apollo-server-micro": "^2.25.0",
      "electron-squirrel-startup": "^1.0.0",
      "graphql": "^15.5.0",
      "next": "10.2.3",
      "next-auth": "^3.25.0",
      "patch-package": "^6.4.7",
      "prisma": "2.24.1",
      "ramda": "^0.27.1",
      "react": "17.0.2",
      "react-dom": "17.0.2"
   },
   "devDependencies": {
      "@babel/core": "^7.14.3",
      "@cypress/code-coverage": "^3.9.6",
      "@cypress/react": "^5.9.1",
      "@cypress/webpack-dev-server": "^1.3.1",
      "@electron-forge/cli": "^6.0.0-beta.54",
      "@electron-forge/maker-deb": "^6.0.0-beta.54",
      "@electron-forge/maker-rpm": "^6.0.0-beta.54",
      "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
      "@electron-forge/maker-zip": "^6.0.0-beta.54",
      "@istanbuljs/nyc-config-typescript": "^1.0.1",
      "@storybook/addon-actions": "^6.2.9",
      "@storybook/addon-essentials": "^6.2.9",
      "@storybook/addon-links": "^6.2.9",
      "@storybook/react": "^6.2.9",
      "@storybook/testing-react": "^0.0.17",
      "@tailwindcss/forms": "^0.3.3",
      "@tailwindcss/typography": "^0.4.1",
      "@types/cypress-cucumber-preprocessor": "^4.0.0",
      "@types/jest": "^26.0.23",
      "@types/mocha": "^8.2.2",
      "@types/node": "^15.12.2",
      "@types/ramda": "^0.27.40",
      "@types/react": "^17.0.9",
      "@typescript-eslint/eslint-plugin": "^4.26.1",
      "@typescript-eslint/parser": "^4.26.1",
      "autoprefixer": "^10.2.6",
      "babel-loader": "^8.2.2",
      "babel-plugin-istanbul": "^6.0.0",
      "concurrently": "^6.2.0",
      "cypress": "^7.5.0",
      "cypress-cucumber-preprocessor": "^4.1.2",
      "cypress-terminal-report": "^3.2.2",
      "dotenv": "^10.0.0",
      "dotenv-cli": "^4.0.0",
      "dotenv-stringify": "^2.0.5",
      "electron": "^12.0.6",
      "eslint": "^7.28.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-plugin-cypress": "^2.11.3",
      "eslint-plugin-jest": "^24.3.6",
      "eslint-plugin-jsx-a11y": "^6.4.1",
      "eslint-plugin-prettier": "^3.4.0",
      "eslint-plugin-react": "^7.24.0",
      "eslint-plugin-react-hooks": "^4.2.0",
      "foreman": "^3.0.1",
      "gherkin-lint": "^4.2.2",
      "html-webpack-plugin": "^5.3.1",
      "husky": "^6.0.0",
      "jest": "^27.0.4",
      "npm-run-all": "^4.1.5",
      "ntl": "^5.1.0",
      "prettier": "^2.3.1",
      "pretty-quick": "^3.1.0",
      "source-map-support": "^0.5.19",
      "start-server-and-test": "^1.12.5",
      "tailwindcss": "^2.1.4",
      "ts-jest": "^27.0.3",
      "ts-node": "^10.0.0",
      "typescript": "^4.3.2",
      "wait-on": "^5.3.0",
      "webpack-dev-server": "^3.11.2"
   }
}
